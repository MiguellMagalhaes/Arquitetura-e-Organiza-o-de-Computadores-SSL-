							#Atividades Práticas em Linguagem Assembly
    		
    		# Exercício "e)" -->  c = a - ( b + d ) 
    		
   .text # começo do programa
   
   	li $s0, 9
   	li $s1, 4
   	li $s3, 2
   	
   		add $t0, $s1, $s3 
   		sub $s2, $s0, $t0
   		
   			li $v0, 1
   			move $a0, $s2
   			
   				syscall