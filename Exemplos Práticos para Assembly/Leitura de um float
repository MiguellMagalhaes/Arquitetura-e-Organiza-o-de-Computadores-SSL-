.data

	msg: .asciiz "Informe um numero decimal: "
	zero: .float 0.0
	
.text

	#print da mensagem para o utilizador
	li $v0, 4
	la $a0, msg
	syscall
	
	#read do número digitado pelo utilizador e save em $f0
	li $v0, 6
	syscall
	
	lwc1 $f1, zero
	add.s $f12, $f1, $f0
	
	#print do número digitado pelo utilizador
	li $v0, 3 #print float
	syscall
	