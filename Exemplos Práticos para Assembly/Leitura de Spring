.data 

	msg1: .asciiz "Qual é o seu nome\n"
	msg2: .asciiz"Olá, "
	nome: .space 50
	
.text
main:

	#print da pergunta
	li $v0, 4
	la $a0, msg1
	syscall
	
	
		#read do nome
		li $v0, 8
		la $a0, nome
		la $a1, 50
		syscall
		
		
			#print da saudação
			li $v0, 4
			la $a0, msg2
			syscall
			
			
				#print do nome
				li $v0, 4
				la $a0, nome
				syscall
				
				
					li $v0, 10
					syscall
