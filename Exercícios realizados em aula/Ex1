.text # f= e - [(a - b) + (b - c)] - Linguagem C

	li $s0, 1
	li $s1, 2
	li $s2, 3
	li $s4, 4
	
		
		sub $t0, $s0, $s1 # (a - b)
		sub $t1, $s1, $s2 # (b - c)
		sub $t2, $t0, $t1 # (a - b) + (b - c)
		add $s5, $s4, $t2 # f = e - [(a - b) + (b - c)]
		
		
				
				
				li $v0, 1 # finalizar o programa 
				move $a0, $s5
				syscall
			
		
			
		
		
